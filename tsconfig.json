{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "typeRoots": [
      "./models",
      "./node_modules/@types"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": ["node_modules", "./node_modules/typescript/lib/lib.es2021.intl.d.ts","/usr/share/code/resources/app/extensions/node_modules/typescript/lib/lib.es2021.intl.d.ts"]
}
